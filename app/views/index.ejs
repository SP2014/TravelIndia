<!DOCTYPE HTML>
<html ng-app="myApp" lang="en" ng-controller="common">
<head>
    <title>{{ pageTitle }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/stylesheets/angular-material.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/trix/0.9.2/trix.css">
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto:400,500,700,400italic'>
    <style type="text/css">
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }
    </style>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css">
</head>

<body ng-cloak>
<div ui-view layout="row" layout-fill>
    <!-- Sidenav -->
    <% include sidenav.ejs %>
    <!-- Main Container -->
    <div flex layout="column" tabIndex="-1" role="main" class="md-whiteframe-z2">
        <% include header.ejs %>
        <md-content id="content" layout-padding>
            <div ng-view></div>
        </md-content>
    </div>

</div>

<script src="/javascripts/angular.js"></script>
<script src="/javascripts/angular-animate.js"></script>
<script src="/javascripts/angular-aria.js"></script>
<script src="/javascripts/angular-material.js"></script>
<script src="/javascripts/angular-route.min.js"></script>
<script src="/scripts/lodash.min.js"></script>
<script src="/scripts/cloudinary-core.min.js"></script>
<script src="/scripts/angular.cloudinary.js"></script>
<script src="/scripts/ng-file-upload.min.js"></script>
<script src="/scripts/ng-file-upload-shim.min.js"></script>
<script src="/scripts/app.js"></script>
<script src="/scripts/index.js"></script>
<script src="/scripts/angular-trix.min.js"></script>
<script src="/services/NavService.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/trix/0.9.2/trix.js"></script>
<script src="/scripts/angular-google-maps-native.min.js"></script>
<script src="/scripts/angular-geocomplete.js"></script>
<script src="/scripts/ngGeolocation.min.js"></script>
<script src="/controllers/appControllers.js"></script>

<script type="text/ng-template" id="form1.tpl.html">
    <gm-infowindow
            options="contentText"
            on-closeclick="console.log('closed #2')">
    </gm-infowindow>
</script>

<script type="text/ng-template" id="maps.tpl.html">
    <gm-map options="setMap">
        <gm-marker gm-address="address" gm-then="map.setCenter(marker.getPosition())" options="{draggable: true}" on-dragend="align(map, marker)"
                   on-position_changed="align(map, marker)">
        </gm-marker>
    </gm-map>
</script>

</body>
</html>